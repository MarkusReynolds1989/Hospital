{
    "$schema": "http://json-schema.org/schema",
    "$id": "None",
    "title": "Config",
    "type": "object",
    "properties": {
        "risk-threshold": {
            "description": "The lower bounds of the risk that should signal an alert.",
            "type": "number",
            "exclusiveMinimum": 1,
            "exclusiveMaximum": 300
        },
        "bmi-threshold": {
            "description": "The lower bounds of the bmi that should signal an alert.",
            "type": "number"
        }
    },
    "required": [
        "risk-threshold",
        "bmi-threshold"
    ]
}
